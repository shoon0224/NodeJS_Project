<!DOCTYPE html>
<html>
<head>
   <title>장난감수정</title>
</head>
<script>
   document.getElementById('endTime').value = data[0].endTime.toISOString().slice(0,-1);
</script>
<style>
   .modify{
      float: left;
   }
</style>
<body>
      <%for(var i=0;i<data.length; i++) { %>
      <form class="modify" action="/toy/modify/<%=data[i].toyId%>" method="POST" enctype="multipart/form-data">
      <h1>경매수정</h1>
      <hr/>
      <br>
      <input type='file' name="photo" value="<%=data[i].toyPic%>" />
      <br>
      <input type="text" id="toyName" name="toyName" value="<%=data[i].toyName%>">
      <br>
      <input type="text" id="toyRprice" name="toyRprice" value="<%=data[i].toyRprice%>">
      <br>
      <input type="text" id="saleReason" name="saleReason" value="<%=data[i].saleReason%>">
      <br>
      <input type="text" id="toyExp" name="toyExp" value="<%=data[i].toyExp%>">
      <br>
      <input type="submit"  value="수정하기">
    </form>
    <% } %>
</body>
</html>